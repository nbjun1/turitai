<div class="container-fluid">
  <%= form_with model: @post, url: post_path, method: :post, local: true do |f| %>
    <div class="row">
      <div class="col offset-5">
        <%= f.label :釣果画像 %>
        <%= f.file_field :image, accept: 'image/*' %>
      </div>
    </div>
    <div class="row">
      <div class="col offset-5">
        <%= f.submit '診断', class: "btn btn-primary" %>
      </div>
    </div>
  <% end %>
</div>
